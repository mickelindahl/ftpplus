{
  "name": "text-file-import",
  "version": "3.0.0",
  "description": "Extends jsftp used fetch multiple file content from a remote directory",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/lab -r lcov test/index.js | ./node_modules/.bin/coveralls",
    "test-lcov": "node_modules/.bin/lab -r lcov test/index.js",
    "test-local": "./node_modules/.bin/lab -m 30000 -l  test/index.js",
    "test-cov-html": "./node_modules/.bin/lab -m 30000 -l -r html -o coverage.html  test/index.js",
    "jsdoc-to-markdown": "jsdoc2md --template README.hbs --files index.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mickelindahl/text_file_import.git"
  },
  "keywords": [
    "ftp",
    "multiple",
    "files"
  ],
  "author": "Mikael Lindahl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mickelindahl/text_file_import/issues"
  },
  "homepage": "https://github.com/mickelindahl/text_file_import#readme",
  "dependencies": {
    "bluebird": "^3.4.6",
    "debug": "^2.3.3",
    "moment": "^2.17.1",
    "sftpjs": "^1.3.4"
  },
  "devDependencies": {
    "code": "^4.0.0",
    "coveralls": "^2.11.16",
    "lab": "^11.2.1"
  }
}
